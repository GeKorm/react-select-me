{
  "name": "react-select-me",
  "description": "A highly scalable react dropdown list",
  "version": "0.8.1",
  "main": "lib/ReactSelectMe.js",
  "author": "Vitalii Maslianok <maslianok@gmail.com> (https://github.com/maslianok)",
  "repository": {
    "type": "git",
    "url": "git://github.com/maslianok/react-select-me.git"
  },
  "bugs": {
    "url": "https://github.com/maslianok/react-select-me/issues"
  },
  "engines": {
    "npm": ">=3"
  },
  "license": "MIT",
  "scripts": {
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "prestart": "npm run build:dll",
    "analyze": "node ./internals/scripts/analyze.js",
    "prepublish": "npm run build",
    "prebuild": "npm run build:clean && npm run test",
    "build:clean": "npm run test:clean && rimraf ./lib",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts -- src ",
    "lint:css": "stylelint ./src/**/*.css",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "build": "npm run build:js && npm run build:css",
    "build:js": "babel src --out-dir lib --ignore src/demo",
    "build:css": "postcss -u postcss-cssnext --postcss-cssnext.browsers \"last 2 versions\" -d lib src/ReactSelectMe.css",
    "start": "cross-env NODE_ENV=development webpack-dashboard -- node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "build:examples": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "pre-commit": "test",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "max-len": [
        2,
        120
      ],
      "import/no-extraneous-dependencies": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-sort-props": 0,
      "react/sort-comp": 0,
      "no-case-declarations": 0,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "arrow-parens": 0,
      "comma-dangle": 0,
      "jsx-a11y/no-static-element-interactions": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": "upper",
      "string-quotes": "single",
      "font-family-name-quotes": "always-where-recommended",
      "selector-pseudo-class-no-unknown": [
        true,
        {
          "ignorePseudoClasses": [
            "global"
          ]
        }
      ],
      "indentation": null
    }
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist"
    ],
    "include": [
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "roots": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|png|ttf|eot|svg|scss)$": "<rootDir>/test/fileMock.js"
    }
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.8",
    "react-virtualized": "^9.7.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "compression": "^1.6.2",
    "coveralls": "^2.13.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "exports-loader": "^0.6.4",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.5",
    "jest-cli": "^19.0.2",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.0",
    "lodash": "^4.14.1",
    "minimist": "^1.2.0",
    "ngrok": "^2.2.2",
    "postcss-cli": "^3.1.1",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.1.3",
    "psi": "^3.0.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "shelljs": "^0.7.7",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.3.3",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.3",
    "immutable": "^3.8.1",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "react-virtualized": "^8.0.0 || ^9.0.0"
  }
}
